<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/login">

    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.mdshahsamir.familytracker.map.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/login"
        android:name="com.mdshahsamir.familytracker.login_register.login.Login"
        android:label="Login"
        tools:layout="@layout/login_fragment">
        <action
            android:id="@+id/action_login_to_mapsFragment"
            app:destination="@id/mapsFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerUser"
        android:name="com.mdshahsamir.familytracker.register.RegisterUser"
        android:label="RegisterUser"
        tools:layout="@layout/register_user_fragment"/>
</navigation>